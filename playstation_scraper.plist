<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>playstation_scraper</string>
    <key>ProgramArguments</key>
    <array>
        <string>/PATH/TO/Project/directory/main.command</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/PATH/TO/Project/directory/main.error.txt</string>
    <key>StandardOutPath</key>
    <string>/PATH/TO/Project/directory/main.log.txt</string>
    <key>StartCalendarInterval</key>
    <dict>
          <key>Hour</key>
          <integer>9</integer>
          <key>Minute</key>
          <integer>0</integer>
    </dict></dict>
</plist>
